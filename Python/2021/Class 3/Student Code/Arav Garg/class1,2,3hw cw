total = 0
for i in range(10):
    num = int(input("Enter number: "))
    total += num

print("The sum of all 10 numbers is", total)


total = 0
count = 0
while count < 10:
    num = int(input("Enter number: "))
    total += num
    count += 1 
print("The sum of all 10 numbers is", total)





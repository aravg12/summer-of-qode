    marks=int(input('please enter your marks'))

if marks <= 50 :

    print('Redo the test')
elif marks>=50 and marks <60 :
    print('E')
elif marks>=60 and marks <70 :
    print('D')
elif marks>=70 and marks <80 :
    print('C')
elif marks>=80 and marks <90 :
    print('B')
    
else :
    print('A')





